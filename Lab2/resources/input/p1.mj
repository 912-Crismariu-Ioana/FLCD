# Compute the max of 3 numbers

# Variable declarations start with the keyword "my"

# Variable identifiers are always preceded by a sigil

my $n1:int, $n2:int, $n3:int, $max:int;
$n1 = stdin();
$n2 = stdin();
$n3 = stdin();
$max = $n1;
if ($n2 > $max) {
    $max = $n2;
};
if ($n3 > $max) {
    $max = $n3;
};
print($max);